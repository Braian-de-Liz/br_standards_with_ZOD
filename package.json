{
  "name": "br_standards_with_zod",
  "version": "1.1.2",
  "description": "Validações de documentos brasileiros (CPF, CNPJ, RG, CEP e Telefone) integradas ao Zod.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "test": "vitest",
    "erro": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "zod",
    "cpf",
    "cnpj",
    "rg",
    "brazil",
    "validation",
    "typescript"
  ],
  "author": "Braian de Liz da Silva",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.0.0",
    "tsup": "^8.3.5",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0",
    "vitest": "^2.1.0"
  },
  "peerDependencies": {
    "zod": "^3.0.0 || ^4.0.0"
  }
}